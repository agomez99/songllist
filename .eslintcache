 ReactAudioPlayer from 'react-audio-player';\n\nclass App extends Component {\n  constructor(props) {\n    super(props);\n    this.ref = firebase.firestore().collection('boards');\n    this.unsubscribe = null;\n    this.state = {\n      boards: []\n    };\n  }\n\n  onCollectionUpdate = (querySnapshot) => {\n    const boards = [];\n    querySnapshot.forEach((doc) => {\n      const { title, author, image, music } = doc.data();\n      boards.push({\n        key: doc.id,\n        doc, // DocumentSnapshot\n        title,\n        author,\n        image,\n        music,\n      });\n    });\n    this.setState({\n      boards\n   });\n  }\n\n  componentDidMount() {\n    this.unsubscribe = this.ref.onSnapshot(this.onCollectionUpdate);\n  }\n\n  render() {\n    return (\n      <div class=\"container\">\n        <div class=\"panel panel-default\">\n          <div class=\"panel-heading\">\n            <h3 style={{textAlign:\"center\", marginTop:\"5%\"}} class=\"panel-title\">\n              SONG LIST\n            </h3>\n          </div>\n          <div style={{marginTop:\"10%\"}} class=\"panel-body\">\n          <h4 style={{textAlign:\"right\"}}><Link to=\"/create\">Add Song</Link></h4>\n\n            <table class=\"table\">\n              <thead>\n                  <th>    </th>\n                  <th>Author</th>\n                  <th>Title</th>\n                  <th>    </th>\n\n              </thead>\n              <tbody>\n                {this.state.boards.map(board =>\n                  <tr>\n                    <td><img src=\"https://files.softicons.com/download/system-icons/lozengue-filetype-icons-by-gurato/png/128/MP3.png\"style={{height:\"70px\"}}/></td>\n                    <td style={{overflow:\"hidden\"}}>{board.author}</td>\n                    <td><Link style={{overflow:\"hidden\"}}to={`/show/${board.key}`}>{board.title}</Link></td>\n                    <td><ReactAudioPlayer\n                    src={board.music}\n                    controls\n                      />\n                  </td>\n                  </tr>\n                )}\n              </tbody>\n            </table>\n\n          </div>\n\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default App;","/home/austineg/Dev/media/my-app/src/components/Show.js",["34"],"import React, { Component } from 'react';\nimport firebase from '../Firebase';\nimport { Link } from 'react-router-dom';\nimport ReactAudioPlayer from 'react-audio-player';\nimport { Col, Container, Row } from 'react-bootstrap';\n\nclass Show extends Component {\n\n  constructor(props) {\n    super(props);\n    this.state = {\n      board: {},\n      key: ''\n    };\n  }\n\n  componentDidMount() {\n    const ref = firebase.firestore().collection('boards').doc(this.props.match.params.id);\n    ref.get().then((doc) => {\n      if (doc.exists) {\n        this.setState({\n          board: doc.data(),\n          key: doc.id,\n          isLoading: false\n        });\n      } else {\n        console.log(\"No such document!\");\n      }\n    });\n  }\n\n  delete(id){\n    firebase.firestore().collection('boards').doc(id).delete().then(() => {\n      console.log(\"Document successfully deleted!\");\n      this.props.history.push(\"/\")\n    }).catch((error) => {\n      console.error(\"Error removing document: \", error);\n    });\n  }\n\n  render() {\n    return (\n   <Container>\n       <Row>\n    <Col>\n        <div style={{display:\"block\", marginLeft:\"auto\",marginRight:\"auto\",width:\"40%\"}} class=\"panel panel-default\">\n          <div class=\"panel-heading\">\n          <h4 style={{textAlign:\"center\"}}>Song Details</h4>\n            <h3 style={{textAlign:\"center\"}} class=\"panel-title\">\n              {this.state.board.title}\n            </h3>\n          </div>\n          <div >\n            <dl style={{textAlign:\"center\"}}>\n              <dt>Author:</dt>\n              <dd>{this.state.board.author}</dd>\n              <dt> Image:</dt>\n\n            <dt><img src=\"https://files.softicons.com/download/system-icons/lozengue-filetype-icons-by-gurato/png/512/MP3.png\"style={{ height:\"300px\", paddingBottom:\"10px\"}}/></dt>\n            <dt><ReactAudioPlayer\n                    src={this.state.board.music}\n                    controls\n                      /></dt>\n            <dt><Link to={`/edit/${this.state.key}`} class=\"btn btn-success\" > Edit</Link>&nbsp;\n            <button onClick={this.delete.bind(this, this.state.key)} class=\"btn btn-danger\">Delete</button></dt>\n            </dl>\n            <div >\n            </div>\n          </div>\n          <h4 style={{textAlign:\"center\"}}><Link to=\"/\">Song List</Link></h4>\n\n        </div>\n        </Col>\n        </Row>\n        </Container>\n    );\n  }\n}\n\nexport default Show;","/home/austineg/Dev/media/my-app/src/index.js",[],"/home/austineg/Dev/media/my-app/src/components/Create.js",["35"],"/home/austineg/Dev/media/my-app/src/Firebase.js",["36"],"\n\nimport firebase from 'firebase';\nimport firestore from 'firebase/firestore'\n\nconst settings = {timestampsInSnapshots: true};\n\nconst config = {\n  apiKey: \"AIzaSyDcbw2GQnqz0igkw7FN0rFpsuM2XLb7ABQ\",\n  authDomain: \"mediadash-de325.firebaseapp.com\",\n  databaseURL: \"YOUR_DATABASE_URL\",\n  projectId: \"mediadash-de325\",\n  storageBucket: \"mediadash-de325.appspot.com\",\n  messagingSenderId: \"462800498813\",\n};\nfirebase.initializeApp(config);\n\nfirebase.firestore().settings(settings);\n\nexport default firebase;","/home/austineg/Dev/media/my-app/src/components/Edit.js",[],{"ruleId":"37","severity":1,"message":"38","line":62,"column":25,"nodeType":"39","endLine":62,"endColumn":160},{"ruleId":"37","severity":1,"message":"38","line":59,"column":17,"nodeType":"39","endLine":59,"endColumn":176},{"ruleId":"40","severity":1,"message":"41","line":2,"column":8,"nodeType":"42","messageId":"43","endLine":2,"endColumn":16},{"ruleId":"40","severity":1,"message":"44","line":4,"column":8,"nodeType":"42","messageId":"43","endLin